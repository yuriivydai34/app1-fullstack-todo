{
  "name": "@todo/api",
  "version": "1.0.0",
  "private": true,
  "description": "Rest API using Node.js, TypeScript ORM Sequelize, Express, Postgres",
  "main": "server.ts",
  "scripts": {
    "build": "tsc",
    "predev": "npm run build",
    "dev": "npx ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:alt": "nodemon --exec ts-node src/server.ts",
    "start": "node dist/server.js",
    "test": "jest",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "orm",
    "sequelize",
    "express",
    "postgres",
    "postgresql",
    "restapi",
    "rest",
    "api",
    "crud"
  ],
  "author": "bezkoder",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "sqlite3": "^5.1.7",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@todo/shared": "1.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "sequelize": "^6.37.1",
    "sequelize-typescript": "^2.1.6"
  }
}
